{
  "name": "hello-world",
  "private": true,
  "sideEffects": false,
  "version": "0.0.0",
  "scripts": {
    "build": "npm run generate-doc && sass app/:app/ --style=compressed --no-source-map && shopify hydrogen build",
    "dev:hydrogen": "shopify hydrogen dev",
    "dev:sass": "sass --watch app/:app/",
    "dev": "npm-run-all --parallel dev:hydrogen dev:sass",
    "preview": "npm run build && shopify hydrogen preview",
    "lint": "eslint --no-error-on-unmatched-pattern --fix --ext .js,.ts,.jsx,.tsx ./app",
    "g": "shopify hydrogen generate",
    "generate-doc": "npx jsdoc -c jsdoc.config.json"
  },
  "lint-staged": {"**/*.{js,jsx,ts,tsx}": ["npm run lint"]},
  "prettier": "@shopify/prettier-config",
  "dependencies": {
    "@remix-run/react": "1.14.0",
    "@shopify/cli": "3.29.0",
    "@shopify/cli-hydrogen": "^4.0.9",
    "@shopify/hydrogen": "^2023.1.6",
    "@shopify/remix-oxygen": "^1.0.4",
    "graphql": "^16.6.0",
    "graphql-tag": "^2.12.6",
    "isbot": "^3.6.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.60.0"
  },
  "devDependencies": {
    "@remix-run/dev": "1.14.0",
    "@shopify/prettier-config": "^1.1.2",
    "eslint": "^8.20.0",
    "eslint-plugin-hydrogen": "0.12.2",
    "husky": "^8.0.3",
    "jsdoc": "^4.0.2",
    "lint-staged": "^13.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4"
  },
  "engines": {
    "node": ">=16.13"
  }
}
